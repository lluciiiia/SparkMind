{
  "name": "spark-mind",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "devx": "pnpm supabase stop && pnpm i && pnpm supabase start && next dev",
    "bdevx": "bun supabase stop && bun i && bun supabase start && next dev",
    "devrs": "pnpm supabase stop --no-backup && pnpm i && pnpm supabase start && pnpm supabase db reset && next dev",
    "bdevrs": "bun supabase stop --no-backup && bun i && bun supabase start && bun supabase db reset && next dev",
    "build": "next build",
    "start": "next start",
    "preview": "next build && next start",
    "lint": "bunx biome check --write ./ || true && bunx stylelint src/styles/**/*.{css,scss,sass} --fix",
    "e2e:headless": "playwright test",
    "e2e:ci": "playwright test --ci",
    "e2e:ui": "playwright test --ui",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "stop": "pnpm supabase stop",
    "sb:status": "pnpm supabase status",
    "sb:start": "pnpm supabase start",
    "sb:stop": "pnpm supabase stop",
    "sb:sync": "pnpm supabase db pull",
    "sb:reset": "pnpm supabase db reset",
    "sb:diff": "pnpm supabase db diff -f new_migration",
    "sb:new": "pnpm supabase migration new new_migration",
    "sb:up": "pnpm supabase migration up",
    "sb:typegen": "pnpm supabase gen types typescript --local --schema public > src/types/supabase.ts && prettier --write src/types/supabase.ts",
    "stripe:login": "stripe login",
    "stripe:listen": "stripe listen --forward-to=localhost:3000/api/webhooks",
    "stripe:fixtures": "stripe fixtures fixtures/stripe-fixtures.json"
  },
